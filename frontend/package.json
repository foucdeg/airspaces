{
  "name": "seed",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mapbox/togeojson": "^0.16.0",
    "@material-ui/core": "^4.0",
    "@material-ui/icons": "^4.0",
    "babel-polyfill": "6.26.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.4",
    "env-cmd": "^9.0.3",
    "formik": "1.3.2",
    "history": "4.7.2",
    "jwt-decode": "^2.2.0",
    "leaflet": "^1.5.1",
    "leaflet-hotline": "^0.4.0",
    "leaflet-rotatedmarker": "^0.2.0",
    "leaflet.gridlayer.googlemutant": "^0.8.0",
    "lodash": "^4.17.12",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-intl": "^3.1.13",
    "react-leaflet": "^2.4.0",
    "react-redux": "^6.0.1",
    "react-router": "^5",
    "react-router-dom": "4.3.1",
    "react-scripts": "^2.1.5",
    "redux": "4.0.1",
    "redux-persist": "5.10.0",
    "redux-saga": "^1.0.2",
    "styled-components": "^4.3",
    "superagent": "4.0.0",
    "typesafe-actions": "^3.0.0"
  },
  "scripts": {
    "start": "REACT_APP_ENV=local react-app-rewired start",
    "build": "env-cmd .env.${REACT_APP_ENV} react-app-rewired build",
    "test": "react-app-rewired test --maxWorkers=4",
    "eject": "react-app-rewired eject",
    "analyze": "env-cmd .env.${REACT_APP_ENV} react-app-rewired build --analyze-bundle",
    "test:coverage": "npm run test -- --coverage",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint-style": "stylelint 'src/**/*.style.{ts,tsx}' 'src/**/style.{ts,tsx}'",
    "deploy": "shipit vps deploy"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/actions.js",
      "!src/**/index.js",
      "!src/**/serviceWorker.js",
      "!src/**/*.wrap.js",
      "!src/index.js",
      "!src/tempPolyfills.js",
      "!src/setupTests.js",
      "!src/redux/reducers.js",
      "!src/redux/sagas.js",
      "!src/redux/store.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "200 kB"
    }
  ],
  "devDependencies": {
    "@types/cheerio": "^0.22.10",
    "@types/classnames": "^2.2.8",
    "@types/enzyme": "^3.1.15",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.12",
    "@types/jwt-decode": "^2.2.1",
    "@types/leaflet": "^1.4.4",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-leaflet": "^2.2.1",
    "@types/react-redux": "^6.0.12",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/styled-components": "^4.1.16",
    "@types/superagent": "^3.8.6",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-plugin-styled-components": "1.8.0",
    "cypress": "^3.4.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "22.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mysticatea": "4.2.4",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "^7.14.3",
    "plop": "2.1.0",
    "prettier": "1.15.1",
    "react-app-rewire-webpack-bundle-analyzer": "1.0.1",
    "react-app-rewired": "2.1.0",
    "react-test-renderer": "16.6.3",
    "redux-saga-test-plan": "^4.0.0-beta.2",
    "shipit": "^1.2.0",
    "shipit-cli": "^4.1",
    "shipit-deploy": "^5.0.0",
    "shipit-yarn": "^0.2.0",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-strict-value": "^1.1.2",
    "stylelint-processor-styled-components": "^1.5.2",
    "typescript": "^3.2.2",
    "wait-on": "^3.2.0",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
